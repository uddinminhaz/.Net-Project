
@{
    ViewBag.Title = "AddBranch";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Add Branch</h2>
    
<form method="post">
    <table>
        <tbody>
            <tr>
                <td><h5>Branch</h5></td>
                <td><input type="text" name="Branch_Location" required /></td>
            </tr>
            <tr>
                <td><h5>Password</h5></td>
                <td><input type="text" name="Pass" required /></td>
            </tr>
            <tr>
                <td colspan="2"><input style="background-color: #3ac4ee;border-radius: 2px;" type="submit" name="Save" value="Confirm" /></td>
            </tr>
        </tbody>
    </table>
</form>

